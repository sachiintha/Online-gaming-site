<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
     
     
           	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.Payments.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/log</url-pattern>
	</servlet-mapping>   
     
       <servlet>
    	<servlet-name>PaymentInsert</servlet-name>
    	<servlet-class>com.Payments.PaymentsInsertServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>PaymentInsert</servlet-name>
    	<url-pattern>/insert</url-pattern>
    </servlet-mapping>
     
          <servlet>
    	<servlet-name>updatePayment</servlet-name>
    	<servlet-class>com.Payments.UpdatePaymentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>updatePayment</servlet-name>
    	<url-pattern>/update</url-pattern>
    </servlet-mapping>
     
          <servlet>
    	<servlet-name>deletePayment</servlet-name>
    	<servlet-class>com.Payments.DeletePaymentsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>deletePayment</servlet-name>
    	<url-pattern>/delete</url-pattern>
    </servlet-mapping>
         
 </web-app>